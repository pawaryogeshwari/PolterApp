<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape android:shape="oval">
                    <corners android:radius="1dp" />
                    <stroke
                        android:width="1dp"
                        android:color="#777" />
                    <gradient
                        android:startColor="#1976D2"
                        android:centerColor="#1976D2"
                        android:endColor="#1976D2"
                        android:angle="270" />
                    <size
                        android:width="@dimen/margin_30"
                        android:height="@dimen/margin_30" />
                </shape>
            </item>

            <item android:drawable="@drawable/ic_tick_selecetd"></item>

