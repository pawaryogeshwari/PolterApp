<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray_color"
    android:orientation="vertical"
    android:paddingTop="@dimen/margin_5"
    android:paddingBottom="@dimen/margin_5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/txtAddLogCounter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/bg_unchecked"
            android:visibility="visible"
            android:textColor="@color/black_color"
            android:padding="@dimen/margin_5"
            android:layout_marginLeft="@dimen/margin_5"
            />

        <LinearLayout
            android:paddingRight="@dimen/margin_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <LinearLayout
                android:id="@+id/llTitleLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:weightSum="1"
                android:visibility="visible">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:textSize="@dimen/text_14sp"
                    android:text="@string/plate_value"
                    android:textColor="#000000"
                    />


                <TextView
                    android:id="@+id/txtAddLogPlate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:textSize="@dimen/text_14sp"
                    android:textColor="#000000"
                    />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/llSpeciesLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:orientation="horizontal"
                android:layout_marginLeft="3dp"
                app:layout_constraintTop_toBottomOf="@id/llTitleLayout"
                >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        android:text="@string/species"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        android:text=" :"/>

                    <TextView
                        android:id="@+id/txtAddLogSpecies"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:weightSum="1">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.60"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        android:text="@string/kind"/>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.10"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        android:text=" :"/>


                    <TextView
                        android:id="@+id/txtAddLogKind"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.30"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        android:text="@string/quality_value" />

                    <TextView
                        android:id="@+id/txtAddLogQuality"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:gravity="center"
                        android:text=""
                        android:maxLines="1"
                        android:ellipsize="end"
                        android:textSize="@dimen/sub_text_font_size"
                        android:textColor="@color/black_color"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llLengthLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:orientation="horizontal"
                android:layout_marginLeft="5dp"
                android:weightSum="1"
                android:visibility="visible"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/length_m"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:id="@+id/txtAddLogLength"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.45"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:gravity="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/diameter_cm"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:id="@+id/txtAddLogDiameter"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.30"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/llSizeLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:orientation="horizontal"
                android:layout_marginLeft="5dp"
                android:weightSum="1"
                android:visibility="visible"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/klasse"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text=" :"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:id="@+id/txtAddLogSize"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:layout_marginLeft="2dp"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.30"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:gravity="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/oversize_m"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:id="@+id/txtAddLogOversize"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.20"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCountLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:orientation="horizontal"
                android:layout_marginLeft="5dp"
                android:weightSum="1"
                android:visibility="visible"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/count_value"
                    android:textSize="@dimen/sub_text_font_size"
                    android:visibility="gone"
                    />

                <TextView
                    android:id="@+id/txtAddLogCount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.50"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.30"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:gravity="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:text="@string/volume_value"
                    android:textSize="@dimen/sub_text_font_size"
                    />

                <TextView
                    android:id="@+id/txtAddLogVolumeM3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.20"
                    android:maxLines="1"
                    android:layout_gravity="end"
                    android:ellipsize="end"
                    android:textColor="@color/black_color"
                    android:textStyle="normal"
                    android:textSize="@dimen/sub_text_font_size"
                    />


            </LinearLayout>


        </LinearLayout>

    </LinearLayout>


</LinearLayout>